{
    "name": "carone/laravel-content",
    "version": "1.0",
    "description": "A light-weight CMS for Laravel projects",
    "keywords": ["laravel", "cms", "content-management", "api"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0",
        "illuminate/database": "^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Carone\\Content\\": "src/"
        },
        "files": [
            "src/Helpers/get_content.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Carone\\Content\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Caron Emile",
            "email": "caron.emile@telenet.be"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Carone\\Content\\CaroneContentServiceProvider"
            ]
        }
    }
}
