{
    "name": "carone/laravel-content",
    "description": "A light-weight CMS for Laravel projects",
    "keywords": ["laravel", "cms", "content-management", "json", "api"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.36",
        "illuminate/database": "^12.36",
        "justinrainbow/json-schema": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Carone\\Content\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Carone\\Content\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Caron Emile",
            "email": "emile.caron@constructiv.be"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Carone\\Content\\CaroneContentServiceProvider"
            ]
        }
    }
}
